{
  "applications": {
    "gecko": {
      "id": "{eb059682-fdfe-4250-a2c2-7f7d1af4fe85}"
    }
  },
  "manifest_version": 2,
  "name": "Pinboard",
  "version" : "0.1.0",
  "description": "A very minimal Google Chrome Extention for Pinboard (http://pinboard.in) that doesn't use API keys, but just opens Pinboard windows.",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "browser_action": {
    "default_title": "Pinboard",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
